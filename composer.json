{
    "name": "hopstudios/hop-docusign",
    "description": "Integrates DocuSign functionalities into your forms.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "hop docusign"
    ],
    "support": {
        "docs": "https://github.com/HopStudios/Hop-Docusign/blob/master/README.md",
        "issues": "https://github.com/HopStudios/Hop-Docusign/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Hop Studios",
            "homepage": "https://www.hopstudios.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "hopstudios\\hopdocusign\\": "src/"
        }
    },
    "extra": {
        "name": "Hop DocuSign",
        "handle": "hop-docusign",
        "developer": "Hop Studios",
        "developerUrl": "https://www.hopstudios.com",
        "documentationUrl": "https://github.com/HopStudios/Hop-Docusign/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/HopStudios/Hop-Docusign/master/CHANGELOG.md",
        "components": {
            "hopDocusignService": "hopstudios\\hopdocusign\\services\\HopDocusignService"
        },
        "class": "hopstudios\\hopdocusign\\HopDocusign"
    }
}
